{
  "canisters": {
    "ledger": {
      "type": "custom",
      "wasm": "ledger.wasm",
      "candid": "ledger.private.did"
    },
    "tokens_transfer": {
      "build": [
        "cargo build --target wasm32-unknown-unknown --package tokens_transfer --release"
      ],
      "candid": "src/tokens_transfer/tokens_transfer.did",
      "type": "custom"
    },
    "rust_backend": {
      "candid": "src/rust_backend/rust_backend.did",
      "package": "rust_backend",
      "type": "rust"
    },
    "user_profile": {
      "candid": "src/user_profile/user_profile.did",
      "package": "user_profile",
      "type": "rust"
    },
    "contract": {
      "candid": "src/contract/contract.did",
      "package": "contract",
      "type": "rust"
    },
    "token": {
      "main": "src/token/main.mo",
      "type": "motoko"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "dfx": "0.9.3",
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  },
  "version": 1
}